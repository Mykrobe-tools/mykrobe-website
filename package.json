{
  "name": "mykrobe-website",
  "productName": "Mykrobe Website",
  "version": "0.6.1",
  "description": "Mykrobe Website",
  "private": true,
  "main": "main.js",
  "scripts": {
    "build-storybook": "build-storybook",
    "check:es": "npm run web-build && npm run es-check es5 ./build/*.js || (echo '\nOne or more build files contains non-es5 code. This is usually caused by a module in the node_modules folder distributing non-transpiled source code. Check recently added or updated modules.' && false)",
    "docs-watch": "documentation serve --watch app/**",
    "docs": "documentation build app/** -f html -o docs",
    "flow": "flow",
    "lint:eslint": "eslint ./",
    "lint:prettier": "prettier -l './**/*.{js,json,css,scss}' || (echo '\nThese files are not formatted correctly. Did you forget to run \"npm run prettier\"?' && false)",
    "lint": "npm run lint:eslint && npm run lint:prettier",
    "precommit": "npm run lint && npm run test && npm run web-build",
    "prettier": "prettier --write './**/*.{js,json,css,scss}'",
    "start": "npm run web-build-server",
    "storybook": "cross-env NODE_ENV=development start-storybook --host 0.0.0.0 --port 6006",
    "test:debug": "echo \"To attach Chrome DevTools, open chrome://inspect/#devices then click 'Open dedicated DevTools for node'\" && node --inspect node_modules/.bin/jest --runInBand",
    "test:e2e": "npm run test -t 'e2e'",
    "test": "jest",
    "web-build-server": "babel-node scripts/build-simple-server.js",
    "web-build": "cross-env NODE_ENV=production webpack --config webpack.config.production.js",
    "web-dev-server": "cross-env NODE_ENV=development webpack-dev-server --hot --host 0.0.0.0 --port 3000 --inline --history-api-fallback --progress --config webpack.config.development.js",
    "web-dev": "npm run web-dev-server",
    "web-open": "open http://localhost:3000"
  },
  "devDependencies": {
    "@storybook/react": "^4.0.9",
    "autoprefixer": "^8.4.1",
    "circular-dependency-plugin": "^5.0.2",
    "clean-webpack-plugin": "^0.1.19",
    "copy-webpack-plugin": "^5.0.1",
    "cross-env": "^5.1.4",
    "css-loader": "^1.0.0",
    "css-modules-require-hook": "^4.2.2",
    "documentation": "^8.0.0",
    "es-check": "^2.0.4",
    "eslint": "^4.19.1",
    "eslint-import-resolver-webpack": "^0.10.0",
    "eslint-plugin-flowtype": "^2.46.1",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-react": "^7.7.0",
    "favicons-webpack-plugin": "^0.0.9",
    "file-loader": "^2.0.0",
    "flow-typed": "^2.3.0",
    "font-awesome": "^4.7.0",
    "history": "^4.7.2",
    "html-webpack-plugin": "^3.0.7",
    "husky": "^0.14.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^22.4.3",
    "makeandship-js-common": "git+ssh://git@bitbucket.org:makeandship/makeandship-js-common#v0.0.99",
    "mini-css-extract-plugin": "^0.4.0",
    "mykrobe-atlas": "git+ssh://git@github.com:mykrobe-tools/mykrobe-atlas#develop",
    "node-sass": "^4.7.2",
    "optimize-css-assets-webpack-plugin": "^5.0.0",
    "postcss-loader": "^3.0.0",
    "prettier": "^1.9.2",
    "prop-types": "^15.6.0",
    "react": "^16.3.0",
    "react-addons-css-transition-group": "^15.6.2",
    "react-cookie": "^1.0.4",
    "react-datepicker": "^1.4.1",
    "react-document-title": "^2.0.3",
    "react-dom": "^16.3.0",
    "react-error-boundary": "^1.2.3",
    "react-markdown": "^4.0.6",
    "react-redux": "^5.0.4",
    "react-router-dom": "^4.2.2",
    "react-router-hash-link": "^1.2.1",
    "react-router-redux": "^5.0.0-alpha.9",
    "react-test-renderer": "^16.3.0",
    "reactstrap": "^6.0.1",
    "redux": "^4.0.0",
    "redux-logger": "^3.0.6",
    "redux-mock-store": "^1.2.1",
    "redux-saga-testing": "^1.0.5",
    "sass-loader": "^7.0.1",
    "selenium-webdriver": "^4.0.0-alpha.1",
    "style-loader": "^0.23.0",
    "uglifyjs-webpack-plugin": "^1.3.0",
    "url-loader": "^1.1.1",
    "url-search-params-polyfill": "^4.0.0",
    "webpack": "^4.17.2",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.4",
    "webpack-merge": "^4.1.2",
    "whatwg-fetch": "^2.0.4"
  },
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.13.1",
    "babel-eslint": "^8.0.2",
    "babel-jest": "^23.0.1",
    "babel-loader": "^7.1.4",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-inline-dotenv": "^1.1.2",
    "babel-plugin-transform-flow-strip-types": "^6.8.0",
    "babel-plugin-transform-runtime": "^6.12.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-minify": "^0.4.1",
    "babel-preset-react": "^6.11.1",
    "babel-preset-react-optimize": "^1.0.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "babel-runtime": "^6.26.0",
    "express": "^4.14.0"
  },
  "engines": {
    "node": "8.12.0"
  },
  "jest": {
    "verbose": true,
    "testURL": "http://localhost/",
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/test/__mocks__/fileMock.js",
      "\\.(css|scss)$": "identity-obj-proxy"
    },
    "transformIgnorePatterns": [
      "/node_modules/(?!(mend-jsonschema|makeandship-js-common|makeandship-api-common|swagger-client)).+\\.js$"
    ]
  }
}
